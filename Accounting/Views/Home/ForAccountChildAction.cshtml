@model Accounting.Models.AccountItemView
@{
    Layout = null;
}

<table class="table table-bordered table-hover">
    <tr>
        <th>#</th>
        <th>類別</th>
        <th>日期</th>
        <th>金額</th>
    </tr>
    
    @foreach (var item in ViewData["lists"] as IList<Accounting.Models.AccountItemView>)
    {
        <tr>
            <td>
                @item.Id
            </td>
            <td>
                @item.Type
            </td>
            <td>
                @item.Datetime
            </td>
            <td>
                @item.Price
            </td>
         </tr>
     }
   
</table>
